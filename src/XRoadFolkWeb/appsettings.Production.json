{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "System": "Warning",
      "System.Net.Http": "Information",
      "System.Net.Http.HttpClient": "Information"
    },
    "Verbose": true,
    "AllowVerboseInProduction": true
  },
  "Features": {
    "ShowLogs": true,
    "Logs": {
      "Enabled": true
    },
    "ResponseViewer": {
      "ShowRawXml": false,
      "ShowPrettyXml": false
    }
  },
  "HttpLogs": {
    "PersistToFile": false
  },
  "Http": {
    "BypassServerCertificateValidation": false
  },
  "IdentityServer": {
    "Clients": {
      "XRoadFolkWeb": {
        "Enabled": false
      }
    }
  }
}
