{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Verbose": false,
    "AllowVerboseInProduction": false
  },
  "Features": {
    "ShowLogs": false,
    "Logs": {
      "Enabled": false
    },
    "ResponseViewer": {
      "ShowRawXml": false,
      "ShowPrettyXml": false
    }
  },
  "HttpLogs": {
    "PersistToFile": false
  },
  "Http": {
    "BypassServerCertificateValidation": false
  },
  "IdentityServer": {
    "Clients": {
      "XRoadFolkWeb": {
        "Enabled": false
      }
    }
  }
}
