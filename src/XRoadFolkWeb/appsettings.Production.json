{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "System": "Warning",
      "System.Net.Http": "Warning",
      "System.Net.Http.HttpClient": "Warning"
    },
    "Verbose": false,
    "AllowVerboseInProduction": false
  },
  "Features": {
    "ShowLogs": true,
    "Logs": {
      "Enabled": true
    },
    "ResponseViewer": {
      "ShowRawXml": false,
      "ShowPrettyXml": false
    }
  },
  "HttpLogs": {
    "PersistToFile": false,
    "Capacity": 300,
    "MaxWritesPerSecond": 50
  },
  "Http": {
    "BypassServerCertificateValidation": false
  },
  "IdentityServer": {
    "Clients": {
      "XRoadFolkWeb": {
        "Enabled": false
      }
    }
  }
}
