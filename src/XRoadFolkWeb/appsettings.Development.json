{
  "DetailedErrors": true,
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Verbose": true
  },
  "HttpLogs": {
    "PersistToFile": true,
    "Capacity": 1000,
    "MaxWritesPerSecond": 200,
    "AlwaysAllowWarningsAndErrors": true,
    "MaxQueue": 5000,
    "FlushIntervalMs": 250
  }
}
